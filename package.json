{
  "name": "deliverai-frontend-web-widget",
  "version": "1.0.0",
  "scripts": {
    "graphql:codegen": "graphql-codegen --config codegen.yml",
    "prestart": "yarn run graphql:codegen",
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "yarn build:prod && http-server dist/application -p 4201 -di --gzip --log-ip",
    "start:proxy:local": "ng serve --proxy-config proxy/local.proxy.conf.json --port 4300",
    "start:proxy:prod": "ng serve --proxy-config proxy/prod.proxy.conf.json --port 4300",
    "prebuild": "yarn run graphql:codegen",
    "build": "ng build",
    "build:watch": "ng build --watch",
    "prebuild:prod": "yarn run graphql:codegen",
    "build:prod": "ng build --prod",
    "prebuild:dev": "yarn run graphql:codegen",
    "build:dev": "ng build --configuration=dev",
    "prebuild:test": "yarn run graphql:codegen",
    "build:test": "ng build --configuration=test",
    "build:elements": "ng build --prod --outputHashing none && node build-script.js",
    "test": "ng test",
    "lint": "yarn lint:angular && yarn lint:scss",
    "lint:angular": "ng lint",
    "lint:scss": "stylelint --config .stylelintrc.js --syntax scss 'src/**/**/*.scss'",
    "docker": "yarn docker:build && yarn docker:run",
    "docker:build": "docker build -t $npm_package_config_docker_image_repository:$npm_package_config_docker_image_tag .",
    "docker:run": "docker run -it --name $npm_package_config_docker_container_name --rm -p $npm_package_config_docker_container_port:80 $npm_package_config_docker_image_repository:$npm_package_config_docker_image_tag",
    "docker:open": "opener http://localhost:$npm_package_config_docker_container_port",
    "e2e": "ng e2e"
  },
  "config": {
    "docker": {
      "image": {
        "repository": "widget-application",
        "tag": "latest"
      },
      "container": {
        "name": "widget-application",
        "port": 14200
      }
    }
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/elements": "9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "^9.1.1",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/fragment-matcher": "^1.15.4",
    "@graphql-codegen/near-operation-file-preset": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-apollo-angular": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ngneat/until-destroy": "^7.1.5",
    "@ngrx/data": "^9.1.0",
    "@ngrx/effects": "^9.1.0",
    "@ngrx/entity": "^9.1.0",
    "@ngrx/router-store": "^9.1.0",
